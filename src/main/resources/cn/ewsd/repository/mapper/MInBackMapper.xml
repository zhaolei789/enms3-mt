<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.ewsd.repository.mapper.MInBackMapper">
	<insert id="insertInBack">
		INSERT INTO m_in_back (`back_id`, `back_step`, `mat_no`, `store_no`, `back_amount`, `back_date`, `back_emp`, `back_info`, `check_no`, `offer_no`, `red_bill`, `init_bill`,
							   `uuid`, `create_time`, `creator`, `creator_id`, `creator_org_id`, `modifier_id`, `modifier`, `modify_time`)
		VALUES (#{backId}, #{backStep}, #{matNo}, #{storeNo}, #{backAmount}, #{backDate}, #{backEmp}, #{backInfo}, #{checkNo}, #{offerNo}, #{redBill}, #{initBill},
				#{uuid}, #{createTime}, #{creator}, #{creatorId}, #{creatorOrgId}, #{modifierId}, #{modifier}, #{modifyTime})
	</insert>
</mapper>